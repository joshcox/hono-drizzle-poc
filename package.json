{
  "name": "bluffcountrybeef",
  "version": "1.0.0",
  "description": "A project for Bluff Country Beef",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "node --inspect --watch dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dk": "drizzle-kit --config ./db/drizzle.config.ts",
    "migrate": "drizzle-kit --config ./db/drizzle.config.ts migrate",
    "migrate:generate": "drizzle-kit --config ./db/drizzle.config.ts generate",
    "migrate:generate:custom": "drizzle-kit --config ./db/drizzle.config.ts generate --custom",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "beef",
    "bluff country"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@hono/zod-validator": "^0.2.2",
    "better-sqlite3": "^11.1.2",
    "drizzle-orm": "^0.32.1",
    "hono": "^4.5.3",
    "reflect-metadata": "^0.2.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^22.1.0",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.23.1",
    "node-gyp": "^10.2.0",
    "ts-node": "^10.9.2",
    "typed-htmx": "^0.3.1",
    "typescript": "^5.5.4"
  }
}
